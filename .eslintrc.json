{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint-config-airbnb", "vue"],
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "no-underscore-dangle": "off"
  }
}
